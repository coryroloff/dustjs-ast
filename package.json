{
  "name": "dustjs-ast",
  "version": "1.0.0",
  "description": "A custom abstract syntax tree for Dust.js",
  "scripts": {
    "test": "nyc --reporter=text --reporter=lcov mocha test/**/*-spec.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint lib test scripts webpack.config.js",
    "fixtures": "babel-node scripts/fixtures.js",
    "docs": "babel-node scripts/docs.js"
  },
  "bin": {
    "dustjs-ast": "bin/dustjs-ast.js"
  },
  "author": "Cory Roloff",
  "license": "MIT",
  "main": "dist/dustjs-ast.js",
  "devDependencies": {
    "alphabet": "^1.0.0",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.12.0",
    "htmlencode": "0.0.4",
    "js-yaml": "^3.6.1",
    "markdown-table": "^0.4.0",
    "mocha": "^2.5.3",
    "nyc": "^6.4.4",
    "pre-commit": "^1.1.3",
    "sinon": "^1.17.4",
    "webpack": "^2.1.0-beta.13",
    "webpack-dev-server": "^2.1.0-beta.0"
  },
  "engines": {
    "node": ">6.x",
    "npm": ">3.x"
  },
  "pre-commit": {
    "run": [
      "lint",
      "test"
    ]
  },
  "dependencies": {
    "commander": "^2.9.0"
  }
}
